# Walmart Social Listener Configuration
# Set these environment variables to optimize performance

# Fast Mode: Skip LLM calls for theme generation (much faster)
export THEMES_FAST_MODE=true

# Embedding Backend: Use TF-IDF instead of sentence transformers (faster)
export THEMES_EMB_BACKEND=tfidf

# OpenAI API Key (optional - only needed for AI-powered theme generation)
# export OPENAI_API_KEY=your_api_key_here
